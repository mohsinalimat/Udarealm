project 'Udarealm.xcodeproj'

# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
inhibit_all_warnings!

target 'Udarealm' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for Udarealm
  pod 'AFNetworking'
  pod 'SDWebImage'
  pod 'PINRemoteImage'
  pod 'Masonry'
  pod 'pop'
  pod 'Realm'
  pod 'AMapLocation'
  pod 'CocoaLumberjack'
  pod 'Firebase/Core'

  pod 'GFBlurView'
  
end



